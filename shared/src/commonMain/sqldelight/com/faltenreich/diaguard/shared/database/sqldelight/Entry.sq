CREATE TABLE entry (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    dateTime TEXT NOT NULL,
    note TEXT
);

create:
INSERT INTO entry(dateTime)
VALUES (?);

getLastId:
SELECT last_insert_rowid();

getById:
SELECT * FROM entry
WHERE id = ?;

getAll:
SELECT * FROM entry
ORDER BY dateTime DESC;

getByQuery:
SELECT * FROM entry
WHERE note LIKE '%' + ? + '%';

update:
UPDATE entry
SET dateTime = ?,
    note = ?
WHERE id = ?;

deleteById:
DELETE FROM entry
WHERE id = ?;